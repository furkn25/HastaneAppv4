@model HastaneAppv4.Models.Doktor

<h2>Yeni Doktor Ekle</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Ad"></label>
        <input asp-for="Ad" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Soyad"></label>
        <input asp-for="Soyad" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Brans"></label>
        <input asp-for="Brans" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Ekle</button>
</form>
